{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "text": "Please provide your date of birth and your email address.",
      "wrap": true
    },
    {
      "type": "Input.Date",
      "label": "Date of Birth",
      "id": "DateOfBirth",
      "validation": {
        "necessity": "Required",
        "errorMessage": "Date of Birth is required"
      }
    },
    {
      "type": "Input.Text",
      "label": "Email Address",
      "id": "EmailAddress",
      "validation": {
        "necessity": "Required",
        "errorMessage": "Email Address is required"
      }
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0",
  "actions": [
    {
      "type": "Action.Submit",
      "title": "OK",
      "data": {
        "button": "DobOK"
      }
    }
  ]
}